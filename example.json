{
"consistency": "deferred",
	"schema": "public",
	"id": "test",
    "tables": [
        {
            "id": "child",
            "key": ["id"],
            "name": "child",
            "schema": "public"
        },
        {
            "id": "parent",
            "dep": "child",
            "join": "parent.id = child.parent_id",
            "name": "parent",
            "schema": "public"
        }
    ],
    "target": {
        "key": ["id"],
        "columns": ["id", "parent_name"],
        "name": "child_full",
        "schema": "public"
    },
    "query": "SELECT c.id, p.name\nFROM $1 AS d\nJOIN child c ON d.id = c.id\nJOIN parent p ON c.parent_id = p.id"
}
